<%- include('layout/header') %>



    <body id="page-top">

        <!-- Page Wrapper -->
        <div id="wrapper">

            <%- include('layout/sidenav') %>

                <!-- Content Wrapper -->
                <div id="content-wrapper" class="d-flex flex-column">

                    <!-- Main Content -->
                    <div id="content">
                        <%- include('layout/topnav') %>






                            <!-- Begin Page Content -->
                            <div class="container-fluid">
                                <div class="maincontainer2">
                                    <% if(message_failed.length) { %>
                                        <div class="alert alert-danger" role="alert">
                                            <%= message_failed %>
                                        </div>
                                        <% } %>
                                            <% if(message_success.length) { %>
                                                <div class="alert alert-success" role="alert">
                                                    <%= message_success %>Now You can go <a href="/admin/viewcategory"
                                                            class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i
                                                                class="fas fa-download fa-sm text-white-50"></i> Show
                                                            Table</a>
                                                </div>
                                                <% } %>
                                                    <% if(message_warning.length) { %>
                                                        <div class="alert alert-warning" role="alert">
                                                            <%= message_warning %>
                                                        </div>
                                                        <% } %>
                                                            <div class="main10">

                                                                <div class="main11">
                                                                    <h1 class="hb1">Enter Your Details</h1>
                                                                </div>
                                                                <form action="/admin/admincategory" method="post"   class="fb">
                                                                   
                                                                    <div class="form-group">
                                                                        <label for="categoryName">Category Name:</label>
                                                                        <input type="text" class="form-control go2"
                                                                        name="categoryName" id="categoryName"
                                                                        placeholder="Enter Your Category Name">

                                                                    </div>
                                                                    <div class="form-group">
                                                                   
                                                                    
                                                                    <button type="submit"
                                                                        class="btn btn-primary btn-user btn-block bt">
                                                                        Category Details Entered
                                                                    </button>
                                                                </form>
                                                            </div>

                                </div>






                            </div>
                            <!-- /.container-fluid -->

                    </div>
                    <!-- End of Main Content -->

                    <%- include('layout/sidefooter') %>


                </div>
                <!-- End of Content Wrapper -->

        </div>
        <!-- End of Page Wrapper -->

        <!-- Scroll to Top Button-->
        <a class="scroll-to-top rounded" href="#page-top">
            <i class="fas fa-angle-up"></i>
        </a>

        <%- include('layout/logoutuser') %>



