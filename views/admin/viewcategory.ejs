<%- include("layout/header") %>

    <body id="page-top">
        <!-- Page Wrapper -->
        <div id="wrapper">

            <%- include("layout/sidenav") %>


                <!-- Content Wrapper -->
                <div id="content-wrapper" class="d-flex flex-column">

                    <!-- Main Content -->
                    <div id="content">

                        <%- include("layout/topnav") %>

                            <!-- Begin Page Content -->
                            <div class="container-fluid">

                                <!-- Page Heading -->
                                <div class="d-sm-flex align-items-center justify-content-between mb-4">
                                    <h1 class="h3 mb-0 text-gray-800">View Categories</h1>
                                    <a href="/admin/addcategoryform"
                                        class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i
                                            class="fas fa-download fa-sm text-white-50"></i> Category Details</a>
                                </div>
                                <div class="card shadow mb-4">
                                    <div class="card-header py-3">
                                        <h6 class="m-0 font-weight-bold text-primary">Blog Categories</h6>
                                    </div>
                                    <div class="card-body">
                                        <div class="table-responsive">
                                            <table class="table table-bordered" id="dataTable" width="100%"
                                                cellspacing="0">
                                                <thead>
                                                    <tr>
                                                        <th>#</th>
                                                        <th>Category Name</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% categoryadmin.map(data=> { %>
                                                        <tr>
                                                            <td>
                                                                <%- data._id %>
                                                            </td>
                                                            <td>
                                                                <%- data.categoryName %>
                                                            </td>

                                                        </tr>
                                                        <% }) %>
                                            </table>
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <!-- End Begin Page Content -->
                    </div>
                    <!-- End of Main Content -->
                    <%- include("layout/logoutuser") %>

                        <%- include("layout/footer") %>